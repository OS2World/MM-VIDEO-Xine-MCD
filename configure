  echo "$as_me:$LINENO: result: $SGMLTOOLS" >&5
echo "${ECHO_T}$SGMLTOOLS" >&6
else
  echo "$as_me:$LINENO: result: no" >&5
echo "${ECHO_T}no" >&6
fi

