   -e 's:$: $lt_compiler_flag:'`
   (eval echo "\"\$as_me:11762: $lt_compile\"" >&5)
   (eval "$lt_compile" 2>conftest.err)
   ac_status=$?
   cat conftest.err >&5
   echo "$as_me:11766: \$? = $ac_status" >&5
